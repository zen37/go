//https://play.golang.org/p/8N696M4sXm3


package main

import (
	"fmt"
)

const multiple_of_3_and_5 string = "FizzBuzz"

const multiple_of_3 string = "Fizz"

const multiple_of_5 string = "Buzz"

func main() {

	for i := 1; i <= 100; i++ {

		if (i%3 == 0) && (i%5 == 0) {

			fmt.Println(multiple_of_3_and_5)

		} else if i%3 == 0 {

			fmt.Println(multiple_of_3)

		} else if i%5 == 0 {

			fmt.Println(multiple_of_5)

		} else {

			fmt.Println(i)

		}

	}

}
